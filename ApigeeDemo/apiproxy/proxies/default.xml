<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>Quota-1</Name>
            </Step>
            <Step>
                <Name>Response-Cache-1</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows>
        <Flow name="Flow-1">
            <Description/>
            <Request/>
            <Response>
                <Step>
                    <Name>JSON-to-XML-1</Name>
                </Step>
            </Response>
            <Condition>response.header.Content-Type = "application/json; charset=UTF-8"</Condition>
        </Flow>
    </Flows>
    <HTTPProxyConnection>
        <BasePath>/apigeedemo</BasePath>
        <Properties/>
        <VirtualHost>default</VirtualHost>
        <VirtualHost>secure</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>